{
  "hosting": {
    "public": "App/dist",
    "predeploy": [
      "npm --prefix .\\app run build"
    ],
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "headers": [
      {
        "source" : "**/*.*",
        "headers" : [ {
          "key" : "Cache-Control",
          "value" : "max-age=604800"
        } ]
      }
    ]
  }
}
